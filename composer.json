{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [{
            "type": "vcs",
            "url": "https://github.com/BadassNinjas/shopkit-core.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BadassNinjas/shopkit-product.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BadassNinjas/shopkit-acl.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BadassNinjas/badass-utils.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BadassNinjas/badass-rfs-client.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "doctrine/dbal": "^2.5",
        "predis/predis": "^1.1",
        "badassninjas/shopkit-core": "^0.7.5",
        "badassninjas/shopkit-product": "^0.2.0",
        "badassninjas/shopkit-acl": "^0.4.0",
        "badassninjas/badass-utils": "^1.0",
        "badassninjas/badass-rfs-client": "^0.6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
